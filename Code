	{
		//Greetings 
		System.out.println("Welcome to Life Planner!");
		System.out.println();
		System.out.println("Press enter to contine...");
		Scanner userInput = new Scanner(System.in);
		System.out.println();
		userInput.nextLine();
	}
}
